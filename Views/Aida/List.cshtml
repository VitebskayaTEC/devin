@using (var db = new DbDevin())
{
    var computers = db.Report
        .Select(x => new Report { ID = x.ID, RHost = x.RHost, RDateTime = x.RDateTime, RUser = x.RUser })
        .OrderBy(x => x.RHost)
        .ToList();

    foreach (var computer in computers)
    {
        @Html.Partial("Computer", computer)
    }
}