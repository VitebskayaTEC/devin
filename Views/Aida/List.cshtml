@using (var conn = Database.Connection())
{
    string sql = @"SELECT ID, RVersion, RHost, RUser, RDateTime 
        FROM Report 
        WHERE ID IN (SELECT Max(ID) FROM Report GROUP BY RHost) 
        ORDER BY RHost";

    var computers = conn.Query<Aida>(sql);

    foreach (var computer in computers)
    {
        @Html.Partial("Computer", computer)
    }
}